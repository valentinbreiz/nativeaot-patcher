<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Only active when RuntimeIdentifier ends with -arm64 -->
  <ItemGroup Condition="$(RuntimeIdentifier.EndsWith('-arm64'))">
    <AsmSearchPath Include="$(MSBuildThisFileDirectory)/IO/" />
    <AsmSearchPath Include="$(MSBuildThisFileDirectory)/CPU/" />
    <AsmSearchPath Include="$(MSBuildThisFileDirectory)/Debug/" />
    <AsmSearchPath Include="$(MSBuildThisFileDirectory)/Runtime/" />
    <AsmSearchPath Include="$(MSBuildThisFileDirectory)/Memory/" />
    <AsmSearchPath Include="$(MSBuildThisFileDirectory)/Devices/" />
  </ItemGroup>
</Project>