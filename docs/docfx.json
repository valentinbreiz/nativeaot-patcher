{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [ "**/*.csproj" ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "src": ".",
        "files": [ "**/*.md", "**/*.yml" ],
        "exclude": [ "_site/**" ]
      }
    ],
    "resource": [
      {
        "src": ".",
        "files": [ "images/**" ]
      }
    ],
    "output": "_site",
    "template": [ "default", "modern" ],
    "globalMetadata": {
      "_appName": "Cosmos Gen 3",
      "_appTitle": "Cosmos Gen 3",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
